<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/main_menu_nav"
            app:startDestination="@id/mainMenuFragment">

    <fragment android:id="@+id/mainMenuFragment" android:name="com.ucsdextandroid2.android2final.MainMenuFragment"
              android:label="fragment_main_menu" tools:layout="@layout/fragment_main_menu">
        <action android:id="@+id/action_mainMenuFragment_to_searchFragment" app:destination="@id/searchFragment"/>
        <action android:id="@+id/action_mainMenuFragment_to_profileFragment" app:destination="@id/profileFragment"/>
    </fragment>
    <fragment android:id="@+id/profileFragment" android:name="com.ucsdextandroid2.android2final.ProfileFragment"
              android:label="ProfileFragment"/>
    <fragment android:id="@+id/searchFragment" android:name="com.ucsdextandroid2.android2final.SearchFragment"
              android:label="SearchFragment"/>
</navigation>